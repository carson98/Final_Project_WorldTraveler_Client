<!-- Header -->
<header class="header-v3">
    <!-- Header desktop -->
    <div class="container-menu-desktop trans-03">
        <div class="wrap-menu-desktop">
            <nav class="limiter-menu-desktop p-l-45">

                <!-- Logo desktop -->
                <a href="/" class="logo">
                    <img src="images/icons/WorldTravelerLogo.png" alt="IMG-LOGO">
                </a>
                <!-- Menu desktop -->
                <div class="menu-desktop">
                    <ul class="main-menu">
                        <li>
                            <a href="/">{{{__ "Home"}}}</a>
                        </li>

                        <li>
                            {{!-- <li class="label1" data-label1="hot"> --}}
                            <a href="/product/product-more">{{{__ "All Tour"}}}</a>
                        </li>
                        <li>
                            <a href="/contact/view">{{{__ "Contact"}}}</a>
                        </li>
                    </ul>
                </div>

                <!-- Icon header -->
                <div class="wrap-icon-header flex-w flex-r-m h-full">
                    <div class="flex-c-m h-full p-r-25 bor6">
                        <ul class="main-menu">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="dropdownId" data-toggle="dropdown"
                                    aria-haspopup="true" aria-expanded="false"><i class="fa fa-user fa-2x"
                                        style="color:rgb(59, 58, 58)" aria-hidden="true"></i></a>
                                <div class="dropdown-menu" aria-labelledby="dropdownId">
                                    {{#if login}}
                                    <a class="dropdown-item" href="/user/profile"><i class="fa fa-user-circle"
                                            aria-hidden="true"></i> {{{__ "User Account"}}}</a>
                                    <a class="dropdown-item" href="/user/logout"><i class="fa fa-sign-out"
                                            aria-hidden="true"></i> {{{__ "Sign Out"}}}</a>
                                    {{else}}
                                    <a class="dropdown-item" href="/user/signin"><i class="fa fa-sign-in"
                                            aria-hidden="true"></i> {{{__ "Sign In"}}} </a>
                                    <a class="dropdown-item" href="/user/signup"><i class="fa fa-user-plus"
                                            aria-hidden="true"></i> {{{__ "Sign Up"}}}</a>
                                    {{/if}}
                                </div>
                            </li>
                        </ul>
                    </div>
                    <div class="flex-c-m h-full p-r-25 bor6">
                        {{#if session.cart.totalQty}}
                        <a href="/cart/shopping-cart/"
                            class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti"
                            data-notify="{{session.cart.totalQty}}">
                            <i class="zmdi zmdi-shopping-cart"></i>
                        </a>
                        {{else}}
                        <a href="/cart/shopping-cart/"
                            class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti"
                            data-notify="0">
                            <i class="zmdi zmdi-shopping-cart"></i>
                        </a>
                        {{/if}}
                    </div>
                    <div class="flex-c-m h-full p-lr-19">
                        <div class="icon-header-item cl0 hov-cl1 trans-04 p-lr-11 js-show-sidebar">
                            <i class="zmdi zmdi-menu"></i>
                        </div>
                    </div>
                </div>
            </nav>
        </div>
    </div>

    <!-- Header Mobile -->
    <div class="wrap-header-mobile">
        <!-- Logo moblie -->
        <div class="logo-mobile">
            <a href="index.html"><img src="images/icons/WorldTravelerLogo.jpg" alt="IMG-LOGO"></a>
        </div>

        <!-- Icon header -->
        <div class="wrap-icon-header flex-w flex-r-m h-full m-r-15">
            <div class="flex-c-m h-full p-r-5">
                <div class="icon-header-item cl2 hov-cl1 trans-04 p-lr-11 icon-header-noti js-show-cart"
                    data-notify="2">
                    <i class="zmdi zmdi-shopping-cart"></i>
                </div>
            </div>
        </div>

        <!-- Button show menu -->
        <div class="btn-show-menu-mobile hamburger hamburger--squeeze">
            <span class="hamburger-box">
                <span class="hamburger-inner"></span>
            </span>
        </div>
    </div>


    <!-- Menu Mobile -->
    <div class="menu-mobile">
        <ul class="main-menu-m">
            <li>
                <a href="/">Home</a>
                <span class="arrow-main-menu-m">
                    <i class="fa fa-angle-right" aria-hidden="true"></i>
                </span>
            </li>

            <li>
                <a href="/product/product-more">All Tour</a>
            </li>

            <li>
                <a href="/contact">Contact</a>
            </li>
        </ul>
    </div>
</header>


