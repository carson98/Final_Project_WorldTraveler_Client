<!-- Banner -->
<div class="sec-banner bg0 p-t-95 p-b-55">
    <div class="container">
        <div class="p-b-10">
            <h3 class="ltext-103 cl5">
                {{__ "Where do you want to go ?"}}
            </h3>
            <hr>
            <p class="mtext-110">
                {{__ "Look for places where you want to go"}}
            </p>
        </div>
        <div class="row mt-3">
            <div class="col-md-6 p-b-30 m-lr-auto">
                <!-- Block1 -->
                <div class="block1 wrap-pic-w">
                    <img src="images/banner-04.jpg" alt="IMG-BANNER">
                    <div class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3"
                        data-toggle="modal" data-target="#searchDomestic">
                        <div class="block1-txt-child1 flex-col-l">
                            <span class="block1-name ltext-102 trans-04 p-b-8">
                                {{__ "SEARCH FOR"}}
                            </span>

                            <span class="block1-info stext-102 trans-04">
                                {{__ "DOMESTIC TOUR"}}
                            </span>
                        </div>

                        <div class="block1-txt-child2 p-b-4 trans-05">
                            <div class="block1-link stext-101 cl0 trans-09">
                                {{__ "Search now"}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6 p-b-30 m-lr-auto">
                <!-- Block1 -->
                <div class="block1 wrap-pic-w">
                    <img src="images/banner-05.jpg" alt="IMG-BANNER">
                    <div class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3"
                        data-toggle="modal" data-target="#searchInternational">
                        <div class="block1-txt-child1 flex-col-l">
                            <span class="block1-name ltext-102 trans-04 p-b-8">
                                {{__ "SEARCH FOR"}}
                            </span>

                            <span class="block1-info stext-102 trans-04">
                                {{__ "INTERNATIONAL TOUR"}}
                            </span>
                        </div>

                        <div class="block1-txt-child2 p-b-4 trans-05">
                            <div class="block1-link stext-101 cl0 trans-09">
                                {{__ "Search now"}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Modal Domestic Search -->
            <div class="modal fade bd-example-modal-lg" id="searchDomestic" tabindex="-1" role="dialog"
                aria-labelledby="myLargeModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content" style="margin-top: 150px;">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalCenterTitle"></h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <form action="/product/product-searchTour" method="post">
                                <div class="form-row">
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <label>DEPART FROM:</label>
                                        <select class="form-control" id="from" name="from">
                                            {{#each departFrom}}
                                            {{!-- <option selected>From</option> --}}
                                            <option value={{this.id}}>{{this.name}}</option>
                                       
                                            {{/each}}
                                        </select>
                                    </div>
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <label>DESTINATION:</label>
                                        <select class="form-control" id="to" name="to">
                                            {{#each desDomestic}}
                                            {{!-- <option selected>To</option> --}}
                                            <option value={{this.id}}>{{this.name}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                    <div class="form-group col-lg-3 col-md-12 col-sm-12 col-xs-12">
                                        <label>DATE:</label>
                                        <input type="date" class="form-control" id="dates" name="dates" />
                                    </div>
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <label>PRICE:</label>
                                        <select class="form-control" id="price" name="price">
                                            <option value="Minh Du" selected>Tất cả</option>
                                            <option value="1">
                                                < 200$</option>
                                            <option value="2">200$ - 500$</option>
                                            <option value="3">> 500$</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <button class="btn btn-md btn-search btn-mg-t" type="submit">
                                            <i class="fa fa-search"></i> Search
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Modal International Search -->
            <div class="modal fade bd-example-modal-lg" id="searchInternational" tabindex="-1" role="dialog"
                aria-labelledby="myLargeModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content" style="margin-top: 150px;">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalCenterTitle"></h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <form action="/product/product-searchTour" method="post">
                                <div class="form-row">
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <label>DEPART FROM:</label>
                                        <select class="form-control" id="from" name="from">
                                            {{#each departFrom}}
                                            {{!-- <option selected>From</option> --}}
                                            <option value={{this.id}}>{{this.name}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <label>DESTINATION:</label>
                                        <select class="form-control" id="to" name="to">
                                            {{#each desInternational}}
                                            {{!-- <option selected>To</option> --}}
                                            <option value={{this.id}}>{{this.name}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                    <div class="form-group col-lg-3 col-md-12 col-sm-12 col-xs-12">
                                        <label>DATE:</label>
                                        <input type="date" class="form-control" id="dates" name="dates" />
                                    </div>
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <label>PRICE:</label>
                                        <select class="form-control" id="price" name="price">
                                            <option selected>Tất cả</option>
                                            <option value="1">
                                                < 200$</option>
                                            <option value="2">200$ - 500$</option>
                                            <option value="3">> 500$</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                        <button class="btn btn-md btn-search btn-mg-t" type="submit">
                                            <i class="fa fa-search"></i> Search
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            {{!-- End row-mt3 --}}
        </div>
    </div>
</div>

{{!-- Parallax --}}
<div class="parallax"></div>

<!-- Product -->
<section class="bg0 p-t-23 p-b-140">
    <div class="container">
        <div class="p-b-10 text-center">
            <h3 class="ltext-103 cl5">
                <img src="images/icons/medal.png" />
                {{__ "All Tours"}}
                <img src="images/icons/medal.png" />
                <div class="text-center title-descript">
                    {{__ "Looking for Domestic and International destinations"}}
                </div>
            </h3>
        </div>

        <div class="flex-w flex-sb-m p-b-52">
            <div class="flex-w flex-l-m filter-tope-group m-tb-10">
                <button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1" data-filter="*">
                    {{__ "All Tours"}}
                </button>

                <button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5" data-filter=".domestic">
                    {{__ "Domestic Tour"}}
                </button>

                <button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5" data-filter=".international">
                    {{__ "International Tour"}}
                </button>
            </div>

            <div class="flex-w flex-c-m m-tb-10">
                <div class="flex-c-m stext-106 cl6 size-105 bor4 pointer hov-btn3 trans-04 m-tb-4 js-show-search">
                    <i class="icon-search cl2 m-r-6 fs-15 trans-04 zmdi zmdi-search"></i>
                    <i class="icon-close-search cl2 m-r-6 fs-15 trans-04 zmdi zmdi-close dis-none"></i>
                    {{__ "Search"}}
                </div>
            </div>

            <!-- Search product -->
            <div class="dis-none panel-search w-full p-t-10 p-b-15">
                <form action="/product-search" method="post">
                    <div class="bor8 dis-flex p-l-15">

                        <button type="submit" class="size-113 flex-c-m fs-16 cl2 hov-cl1 trans-04">
                            <i class="zmdi zmdi-search"></i>
                        </button>

                        <input class="mtext-107 cl2 size-114 plh2 p-r-15" type="text" name="search" id="search"
                            placeholder="Search">

                    </div>
                </form>
            </div>
        </div>
        {{# each products}}
        <div class="row isotope-grid">
            {{# each this}}
            <div class="col-sm-6 col-md-4 col-lg-4 p-b-35 isotope-item {{this.userGroup}}">
                <!-- Block2 -->
                <div class="block2">
                    <div class="block2-pic hov-img0">
                        <img src="images/{{this.imagePath}}" alt="IMG-PRODUCT">
                        {{!-- js-show-modal1 into class --}}
                        <a href="/product/detail/{{this._id}}"
                            class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04 ">
                            {{__ "Quick View"}}
                        </a>
                    </div>

                    <div class="block2-txt flex-w flex-t p-t-14">
                        <h4 class="price">
                            <span>
                                $ {{this.price}}
                            </span>
                        </h4>
                        <div class="block2-txt-child1 flex-col-l">
                            <a href="/product/detail/{{this._id}}"
                                class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
                                <h5>{{this.title}}</h5>
                            </a>
                        </div>

                        <div class="block2-txt-child2 flex-r p-t-3">
                            <a href="#" class="btn-addwish-b2 dis-block pos-relative js-addwish-b2">
                                <img class="icon-heart1 dis-block trans-04" src="../images/icons/icon-heart-01.png"
                                    alt="ICON">
                                <img class="icon-heart2 dis-block trans-04 ab-t-l"
                                    src="../images/icons/icon-heart-02.png" alt="ICON">
                            </a>
                        </div>
                        <div class="tgc-line"></div>
                        <div class="content">
                            <div class="row">
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p class="p-1"><i class="zmdi zmdi-flight-takeoff"></i> Depart:
                                        <strong>{{this.depart.name}}</strong>
                                    </p>
                                    <p class="p-1"><i class="zmdi zmdi-calendar"></i> Date:
                                        <strong>{{this.date}}</strong></p>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <p class="p-1"><i class="zmdi zmdi-airline-seat-recline-extra"></i> Seat
                                        Availability:
                                        <strong>{{this.seat}}</strong></p>
                                    <p class="p-1"><i class="zmdi zmdi-time"></i> Durations:
                                        <strong>{{this.duration}}
                                            days</strong></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/each}}
        <!-- Load more -->
        <div class="flex-c-m flex-w w-full p-t-45">
            <a href="/product/product-more" class="flex-c-m stext-101 cl5 size-103 bg2 bor1 hov-btn1 p-lr-15 trans-04">
                {{__ "Load More"}}
            </a>
        </div>
    </div>
</section>


{{>footer}}